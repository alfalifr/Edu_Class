<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sidev.kuliah.tekber.edu_class">

    <uses-permission android:name="android.permission.INTERNET" />
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_graduate_logo"
        android:label="@string/app_name_"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name="sidev.kuliah.tekber.edu_class.app.App"
        android:theme="@style/AppTheme">

        <!-- MAIN -->
        <activity android:name=".act.StudentMainAct"/>
        <activity android:name=".act.TeacherMainAct"/>
        <!-- MAIN end-->

        <activity android:name=".act.SimpleSingleFragActImpl" />
        <activity android:name=".act.ContentAct" />

        <activity android:name="sidev.kuliah.tekber.edu_class.act.LoginAct" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>